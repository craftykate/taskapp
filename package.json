{
  "name": "todos-offline",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.39",
    "@types/react": "^16.14.21",
    "@types/react-dom": "^16.9.14",
    "env-cmd": "^10.1.0",
    "idb-keyval": "^6.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "sass": "^1.45.1",
    "typescript": "^4.5.4",
    "web-vitals": "^0.2.4",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-streams": "^5.1.4"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "start:local": "env-cmd -f .env.local react-scripts start",
    "start:prod": "env-cmd -f .env.prod react-scripts start",
    "build": "react-scripts build",
    "build_local": "react-scripts build && mkdir -p builds && mv -f build builds/local",
    "build_prod": "react-scripts build && cp build/index.html build/200.html && mkdir -p builds && mv -f build builds/prod",
    "build:local": "env-cmd -f .env.local npm run-script build_local",
    "build:prod": "env-cmd -f .env.prod npm run-script build_prod"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
